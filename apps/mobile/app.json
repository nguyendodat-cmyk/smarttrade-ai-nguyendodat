{
  "expo": {
    "name": "SmartTrade AI",
    "slug": "smarttrade-mobile",
    "version": "1.5.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "smarttrade",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nclamvn.smarttrade"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0A0A"
      },
      "package": "com.nclamvn.smarttrade",
      "versionCode": 26
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "41d5c0ab-bff6-44d6-98c1-0590ea4245f8"
      }
    },
    "owner": "nclamvn",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/41d5c0ab-bff6-44d6-98c1-0590ea4245f8"
    }
  }
}
